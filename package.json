{
  "name": "GOOD-Developer",
  "version": "0.1.0",
  "description": "Blog about programming best practices",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:hopeisnear/blog.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Rafal Szczepankiewicz",
  "license": "MIT",
  "keywords": [
    "blog",
    "best practices",
    "good developer",
    "sanity",
    "react"
